{
  "routes": [
    {
      "src": "/service-worker.js",
      "headers": {
        "cache-control": "no-cache, no-store, must-revalidate",
        "Service-Worker-Allowed": "/"
      }
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, must-revalidate"
        }
      ]
    }
  ]
}